{
	"name": "srfm/address-compact",
	"title": "Address Compact",
	"description": "Displays a SureForm Address Compact field.",
	"category": "sureforms",
	"keywords": [ "address", "country", "location", "field", "compact" ],
	"textdomain": "sureforms",
	"supports": {
		"reusable": false,
		"html": false
	},
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"attributes": {
		"preview": {
			"type": "boolean",
			"default": false
		},
		"block_id": {
			"type": "string"
		},
		"required": {
			"type": "boolean"
		},
		"fieldWidth": {
			"type": "number",
			"default": 100
		},
		"label": {
			"type": "string",
			"default": "Address"
		},
		"errorMsg": {
			"type": "string",
			"default": ""
		},
		"lineOnePlaceholder": {
			"type": "string",
			"default": "Address Line 1"
		},
		"lineTwoPlaceholder": {
			"type": "string",
			"default": "Address Line 2"
		},
		"cityPlaceholder": {
			"type": "string",
			"default": "City"
		},
		"statePlaceholder": {
			"type": "string",
			"default": "State / Province / Region"
		},
		"postalPlaceholder": {
			"type": "string",
			"default": "Postal Code"
		},
		"countryPlaceholder": {
			"type": "string",
			"default": "Select Country"
		},
		"formId": {
			"type": "number",
			"default": ""
		},
		"help": {
			"type": "string",
			"default": ""
		},
		"isConditionalLogic": {
			"type": "boolean",
			"default": false
		},
		"slug": {
			"type": "string",
			"default": ""
		}
	}
}
